#include "dog.h"
#include <stdio.h>
#include <stdlib.h>
/**
 *free_dog - print a dog
 *@name: th dog
 *@age: som age
 *@owner: some owner
(* a blank line
*Description: smiley is my dog my dog)?
(* section header: header of this dog is dog.h)*
 *Return: nothing
*/
void free_dog(dog_t *d)
{
	free(d->name);
	free(d->owner);
	free(d);
}
