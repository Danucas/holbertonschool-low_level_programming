#include <stdlib.h>
#include <stdio.h>
#include <time.h>
/**
 *main - prints depends on negative positive or zero
 *Return: 0 for great execution
 */
int main(void)
{
char alpha = 'a';
int i = 0;
while (i < 52)
{
if (i <= 25)
{
putchar(alpha + i);
}
else
{
putchar(i + 39);
}
i++;
}
putchar('\n');
return (0);
}
